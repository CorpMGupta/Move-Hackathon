              </td>
            </tr>
            <tr>
              <td>Comments</td>
              <% if !session[:user_id].nil? %>
              <td>
                <table border="1" cellspacing="0" cellpadding="0">
                <% Rating.all.each do |f| %>
                  <% if f.tid.eql?(@team.id) && !f.jid.nil? %>
                      <tr>
                        <td><%= Judge.all.find(f.jid).name %></td>
