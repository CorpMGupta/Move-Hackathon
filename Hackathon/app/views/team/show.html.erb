                    <% if !session[:user_id].nil? %>
                        <p style="width: 18%; float: left; padding-top: 20px;">Comment</p><p style="width:81%;float: left; vertical-align: middle;"><%= f.text_area :comment, :id => "comments_area" %></p>
                    <% end %>

                    <p><%= f.hidden_field :tid, { :value => @team.id} %></p>

                    <% session[:team_id] = @team.id %>

